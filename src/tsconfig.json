{
	"compilerOptions": {
		"target": "es2021",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"useDefineForClassFields": true,
		"experimentalDecorators": true,
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
			"browser/*": ["./*"],
			"react-dom": ["./node_modules/@dothq/react-dom"]
		}
	}
}
