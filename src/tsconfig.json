{
	"compilerOptions": {
		"target": "es2021",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"useDefineForClassFields": true,
		"experimentalDecorators": true,
		"jsx": "preserve",
		"jsxImportSource": "@emotion/react",
		"baseUrl": ".",
		"paths": {
			"browser/*": ["./*"]
		}
	}
}
